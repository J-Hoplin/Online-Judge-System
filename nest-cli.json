{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "aws-s3": {
      "type": "library",
      "root": "libs/aws-s3",
      "entryFile": "index",
      "sourceRoot": "libs/aws-s3/src",
      "compilerOptions": {
        "tsConfigPath": "libs/aws-s3/tsconfig.lib.json"
      }
    },
    "judge0": {
      "type": "library",
      "root": "libs/judge0",
      "entryFile": "index",
      "sourceRoot": "libs/judge0/src",
      "compilerOptions": {
        "tsConfigPath": "libs/judge0/tsconfig.lib.json"
      }
    }
  }
}