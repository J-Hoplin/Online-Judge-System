// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id       String @default(uuid()) @id
  
  nickname     String @unique

  password String

  email    String @unique

  message  String? 

  github   String?
  
  blog     String?

  @@map("user")
}

enum UserType {
  Contributer
  User
}